    package="com.hszl.medicine">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        android:name=".app.App"
    >
        <activity android:name=".activity.BaseActivity"></activity>
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MedicineActivity"></activity>
        <activity android:name=".activity.MedicineInfoActivity"></activity>
        <activity android:name=".activity.MedicineListActivity"></activity>
        <activity android:name=".activity.MedicineTypeActivity"></activity>
        <activity android:name=".activity.StockInActivity"></activity>
        <activity android:name=".activity.StockInfoActivity"></activity>
        <activity android:name=".activity.SupplierInfoActivity"></activity>
        <activity android:name=".activity.SupplierTypeActivity"></activity>
        <activity android:name=".activity.UpdateMedicineActivity"></activity>
        <activity android:name=".activity.UpdateMedicineInfoActivity"></activity>
        <activity android:name=".activity.UpdateStockActivity"></activity>
        <activity android:name=".activity.UpdateSupplierInfo"></activity>
        <activity android:name=".activity.UpdateSupplierTypeActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".activity.SaveMedicineTypeActivity"></activity>
        <activity android:name=".activity.ChooseMedicineTypeActivity"></activity>
        <activity android:name=".activity.StatisticsReportActivity"></activity>
        <activity android:name=".activity.StockMedicineReportActivity"></activity>
        <activity android:name=".MainActivity1"></activity>
    </application>

</manifest>